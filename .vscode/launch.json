{
    "version": "0.2.0",
    "configurations": [

      {
        "type": "node",
        "request": "launch",
        "name": "standardExample.js",
        "program": "${workspaceFolder}\\examples\\standardExample.js",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/**/*.js"],
        "sourceMaps": true,
        "protocol": "inspector",
        "restart": true
      },

      {
        "type": "node",
        "request": "launch",
        "name": "allPartials.js",
        "program": "${workspaceFolder}\\scripts\\allPartials.js",
        "console": "integratedTerminal",
        "internalConsoleOptions": "neverOpen",
        "skipFiles": ["<node_internals>/**"],
        "outFiles": ["${workspaceFolder}/**/*.js"],
        "sourceMaps": true,
        "protocol": "inspector",
        "restart": true
      }
    ]
  }
  